{
  "name": "geek-tools",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*",
    "templates/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset"
  },
  "devDependencies": {
    "prettier": "^2.5.1",
    "turbo": "latest",
    "@changesets/cli": "^2.19.0"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
